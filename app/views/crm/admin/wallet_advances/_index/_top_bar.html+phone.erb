<div class="as-bar">
  <%= link_to({ controller: 'wallets' }, class: 'px-4') do %>
    <%= svg_tag 'angle-left', kind: 'regular' %>
  <% end %>
  <div class="bar-title">
    <span><%= @maintain.remark %></span>
    <% if @wallet.wallet_template_id %>
      <span class="ml-1"><%= @wallet.wallet_template.name %></span>
    <% else %>
      <span class="ml-1">人民币钱包</span>
    <% end %>
  </div>
  <i class="px-5"></i>
</div>
