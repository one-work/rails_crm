<tr id="tr_<%= model.id %>" data-controller="show">
  <%= yield %>
  <td>
    <div data-show-target="item" class="visibility-hidden">
      <%= button_to({ controller: 'trade/admin/card_templates', action: 'show', id: model.id }, aria: { label: t('.card_templates.show.title') }, class: 'button is-small is-borderless') do %>
        <%= svg_tag 'eye', class: 'icon text-info' %>
      <% end %>
    </div>
  </td>
</tr>
