<div class="box is-padding-less">
<% @sources.each do |source| %>
  <%= link_to({ action: 'source', source_id: source.id }, class: 'media') do %>
    <div class="media-content"><%= source.name %></div>
    <div class="media-right">
      <%= svg_tag 'chevron-right' %>
    </div>
  <% end %>
<% end %>
</div>
